{
    "name": "Project Axeman",
    "description": "__MSG_extensionDescription__",
    "version": "0.2.8",
    "manifest_version": 2,
    "default_locale": "en",
    "homepage_url": "http://github.com/JustBuild/Project-Axeman",
    "icons": {
        "128": "Icons/icon128.png",
        "16": "Icons/icon16.png",
        "48": "Icons/icon48.png"
    },
    "permissions": [
        "notifications",
        "storage",
        "unlimitedStorage",
        "tabs"
    ],
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
    "background": {
        "page": "Pages/Background/Background.html",
        "persistent": false
    },
    "options_page": "Pages/PluginsManager/PluginsManager.html",
    "browser_action": {
        "default_icon": "Icons/icon48.png",
        "default_title": "Project Axeman",
        "default_popup": "Pages/Popup/Popup.html"
    },
    "content_scripts": [
        {
            "run_at": "document_end",
            "js": [
                "Scripts/jQuery.js",
                "Scripts/jQuery.sortElements.js",
                "Scripts/jQueryUI.js",
                "Utils/pageslide/jquery.pageslide.js",
                "Utils/underscore/underscore.js",
                "Scripts/App/Enums.js",
                "Scripts/App/Variables.js",
                "Scripts/App/Helpers.js",
                "Scripts/App/Models.js",
                "Scripts/App/RequestManager.js",
                "Scripts/App/NotificationManager.js",
                "Scripts/App/NavigationManager.js",
                "Scripts/App/PluginsManager.js",
                "Plugins/Core/Services/Services.js",
                "Plugins/Fun/Decorations/Decorations.js",
                "Plugins/Support/Feedback/Feedback.js",
                "Plugins/Economy/ResourceIndicator/ResourceIndicator.js",
                "Plugins/Economy/ResourceCalculator/ResourceCalculator.js",
                "Plugins/Economy/UpgradeIndicator/UpgradeIndicator.js",
                "Plugins/Economy/ResourceSender/ResourceSender.js",
                "Plugins/Utility/VillageListEnhancement/VillageListEnhancement.js",
                "Plugins/Utility/MarketplaceEnhancements/MarketplaceEnhancements.js",
                "Plugins/Utility/ReportEnhancement/ReportEnhancement.js",
                "Plugins/Utility/TravianPLUS/TravianPLUS.js",
                "Plugins/Utility/AutoAdventure/AutoAdventure.js",
                "Plugins/Development/_Development/_Development.js",
                "Scripts/App/App.js",
                "Scripts/App/AppLauncher.js"
            ],
            "css": [
                "Pages/jQueryUIStyle.css",
                "Pages/PAStyle.css",
                "Utils/pageslide/jquery.pageslide.css"
            ],
            "matches": [
                "http://*.travian.ae/*",
                "http://*.travian.asia/*",
                "http://*.travian.com.au/*",
                "http://*.travian.ba/*",
                "http://*.travian.bg/*",
                "http://*.travian.com.br/*",
                "http://*.travian.cl/*",
                "http://*.travian.cc/*",
                "http://*.travian.cz/*",
                "http://*.travian.de/*",
                "http://*.travian.dk/*",
                "http://*.travian.co.ee/*",
                "http://*.travian.com.eg/*",
                "http://*.travian.com/*",
                "http://*.travian.net/*",
                "http://*.travian.fi/*",
                "http://*.travian.fr/*",
                "http://*.travian.gr/*",
                "http://*.travianteam.com/*",
                "http://*.travian.hk/*",
                "http://*.travian.com.hr/*",
                "http://*.travian.hu/*",
                "http://*.travian.co.id/*",
                "http://*.travian.co.il/*",
                "http://*.travian.in/*",
                "http://*.travian.ir/*",
                "http://*.travian.it/*",
                "http://*.travian.jp/*",
                "http://*.travian.kr/*",
