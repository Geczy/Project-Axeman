                "http://*.travian.pt/*",
                "http://*.travian.ro/*",
                "http://*.travian.rs/*",
                "http://*.travian.ru/*",
                "http://*.travian.sa/*",
                "http://*.travian.se/*",
                "http://*.travian.si/*",
                "http://*.travian.sk/*",
                "http://*.travian.sy/*",
                "http://*.travian.th/*",
                "http://*.travian.tr/*",
                "http://*.travian.tw/*",
                "http://*.travian.com.ua/*",
                "http://*.travian.co.uk/*",
                "http://*.travian.us/*",
                "http://*.travian.com.vn/*",
                "http://*.travian.co.za/*"
